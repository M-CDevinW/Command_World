{
    "type": "origins:action_on_callback",
    "entity_action_chosen": {
		"type": "origins:and",
		"actions": [
		{
			"type": "origins:execute_command",
			"command": "scale set pehkui:jump_height 1.75 @s"
		},
		{
			"type": "origins:execute_command",
			"command": "scale set pehkui:falling 0.66 @s"
		},
		{
			"type": "origins:execute_command",
			"command": "scale persist set true @s"
		}
		]
    },
    "entity_action_lost": {
        "type": "origins:execute_command",
        "command": "scale reset @s"
    },
	"name": "High Ground",
	"description": "Your experience in traveling the forest has gifted you with increased jump height and reduced fall damage."
}